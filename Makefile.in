# This is a special tar target because it builds not only the tar file for
# the core software but also the lib, etc, and bin directories. It assumes
# that the root directory for DODS is called `DODS' (no version number).
.PHONY: tar
tar:
	-rm -r config.cache config.log config.status
	cd $(DODS_ROOT).. && \
	$(TAR) --exclude 'nc3-dods*' --exclude 'jg-dods*' \
	  --exclude 'dsp-dods*' --exclude 'math-dods*' --exclude 'hdf-dods*' \
	  --exclude 'ff-dods*' --exclude 'tools' --exclude 'clients' \
	  --exclude 'packages*' \
	  --exclude 'bin' --exclude 'lib' --exclude 'include' \
	  --exclude 'man' --exclude 'CVS' --exclude 'old' \
	  --exclude '.#*' --exclude '.flc' --exclude '.snprj' \
	  --exclude 'Makefile' \
	  --gzip --create --dereference --file DODS-$(dir).tar.gz DODS 

.PHONY: binary-tar
binary-tar:
	if [ ! -x $(DODS_ROOT)etc/asciival \
	    -o ! -x $(DODS_ROOT)etc/www_int \
	    -o ! -x $(DODS_ROOT)etc/nph-dods \
	    -o ! $(DODS_ROOT)etc/check_perl.sh ]; then \
	    echo "Install asciival, www_int and nph-dods; remove perl debugging!"; \
	else \
	    cd $(DODS_ROOT).. && \
	    ($(TAR) --gzip --create --dereference \
		--file DODS-dap-library-$(version)-$(host).tar.gz \
		--exclude 'CVS' \
		DODS/NEWS \
		DODS/docs/README DODS/docs/README-dap \
		DODS/bin/opendap-config \
		DODS/lib/libdap++.a \
		DODS/include ; \
	    $(TAR) --gzip --create --dereference \
		--file DODS-client-tools-$(version)-$(host).tar.gz \
		DODS/NEWS \
		DODS/docs/README DODS/docs/README-client-tools \
		DODS/etc/INSTALL-clients \
		DODS/bin/geturl ; \
	    $(TAR) --gzip --create --dereference \
		--file DODS-server-tools-$(version)-$(host).tar.gz \
		--exclude 'CVS' \
		DODS/NEWS \
		DODS/docs/README DODS/docs/README-server-tools \
		DODS/etc/curl \
		DODS/etc/INSTALL-servers \
		DODS/etc/installServers \
		DODS/etc/nph-dods \
		DODS/etc/dods.rc \
		DODS/etc/asciival \
		DODS/etc/www_int \
		DODS/etc/deflate \
		DODS/etc/usage \
		DODS/etc/*.pm \
		DODS/etc/HTML); \
	fi

.PHONY: update-version
update-version: check-version
	@echo "Version is: `cat version.h`"
	if test ! -d ../$(dir); then mv ../$(basedir)* ../$(dir); fi
	if test ! -f $(DODS_ROOT)VERSION; then touch $(DODS_ROOT)VERSION; fi
	$(etcdir)/update-manifest.pl "$(COMPONENT_NAME)" $(version) \
		< $(DODS_ROOT)VERSION > tmp.dods.manifest
	mv tmp.dods.manifest $(DODS_ROOT)VERSION

# Compare the version encoded in this Makefile (set by configure) with the
# version in version.h. The version make variable is set up near the top of
# the Makefile.
.PHONY: check-version
check-version:
	@echo "Checking for version.h and Makefile version match-up"
	@if [ "$(version)" != "`cat version.h`" ]; \
	then \
		echo "You must manually re-run configure!"; \
		exit 1; \
	else \
		echo "Yes, they match."; \
	fi

.PHONY: tags
tags:
	$(TAGS) $(TAGS_FLAGS) *.cc *.h *.lex *.y

.PHONY: depend
depend: 
	@depend@ -m Makefile.in -- $(CPPFLAGS) -- \
	$(CLIENTSRCS) $(DASSRCS) $(DDSSRCS) $(EXPRSRCS) $(TYPESRCS) \
	$(ERRORSRCS) $(AISSRCS) $(TESTSRCS) $(TESTPROGSRCS) $(UTILSRCS) \
	$(UNIT_TEST_SRCS) $(AISSRCS) $(DDXSRCS) $(GNUSRCS)
	-(cd tests && $(MAKE) $(MFLAGS) depend)
	$(MAKE) $(MFLAGS) Makefile

# Once the unit tests are working, add this to the sources on which we run
# depend. 
# $(UNIT_TEST_SRC)

# Build GNU regex 0.12 if we need the object file. The Regex class
# (GNU/Regex.cc) uses the regex 0.12 library, which is just a single object
# file. I've included the library in this source directory; if we need the
# code (some version of Linux include it in libc) then regex.o will be
# included in dependencies above (See GNUOBJS). 04/09/04 jhrg
# 
# I modified this so that only regex.o is built. This avoids potential
# problems with the docs not building because TeX is not present or
# configured correctly. 04/12/04 jhrg

@REGEX_O@ : @REGEX_C@
	cd regex-0.12 && ./configure && make regex.o

.SUFFIXES:      .o .cc .c

.c.o:
	$(CC) -c $(CFLAGS) $(CPPFLAGS) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(CPPFLAGS) -o $@ $<

